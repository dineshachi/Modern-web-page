*
{
margin:0;
padding:0;
}
.header
{
	width:100%;
	min-height:100vh;
	background-image:url('../p3.png');
	background-size:cover;
	background-position:center;
	position:relative;
	background-repeat:no-repeat;
}
.icon
{
	position:absolute;
	top:0;
	right:0;
	width:120px;
	height:100vh;
	background:rgba(0,2,63,0.9);
	text-align:center;
}
.icon a
{color:#fff;
font-size:20px;
width:22px;
display:block;
margin:50px auto 80px;
cursor:pointer;
}
.social
{
	width:100%;
	height:50%;
	background:#ed383d;
	position:absolute;
	bottom:0;
	display:flex;
	align-items:center;
	flex-direction:column;
	justify-content: flex-end;
	padding-bottom:30px;
}
.logo1
{
	position:absolute;
	bottom:85%;
}
